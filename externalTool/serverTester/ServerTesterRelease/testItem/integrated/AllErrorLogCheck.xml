            <help>User 서비스에 에러 로그가 존재하는지 확인합니다.</help>

            <method>GET</method>
            <baseUrl>http://localhost:8088</baseUrl>
            <resourceUrl>api/user/sanityCheck/logs?regFilter=.*ERROR.*</resourceUrl>
            <headers></headers>
            <data></data>

            <checks>
                <check type="status">2.*</check>
                <check type="data">"logs"[ ]*:[ ]*\[[ ]*\]</check>
            </checks>
        </test>

